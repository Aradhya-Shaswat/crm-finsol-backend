{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/download/(.*)",
        "destination": "/download/$1"
      },
      {
        "source": "/public/(.*)",
        "destination": "/public/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ],
    "trailingSlash": false
  }
  